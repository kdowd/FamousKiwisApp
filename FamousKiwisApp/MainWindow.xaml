<Window x:Class="FamousKiwisApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FamousKiwisApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1000" Initialized="OnInitialized">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="0.547"/>
            <GradientStop Color="White" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid ShowGridLines="False" >
        <Frame x:Name="NavFrame" Source="/Pages/Navigation/MainMenu.xaml" Panel.ZIndex="2"  />
        <Frame Margin="0 40 0 0 " x:Name="MainAppFrame" Source="/Pages/AllKiwis.xaml" NavigationUIVisibility="Hidden" Panel.ZIndex="1" />

    </Grid>
</Window>
